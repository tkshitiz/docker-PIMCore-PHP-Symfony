									    "title": "Drag your image here",
									    "width": 100,
									    "height": 100,
									    "thumbnail": "instagram_icon"
									}) }}
					{% else %}
						<img src="{{pimcore_image("instagram_icon").getSrc() }}" alt="{{pimcore_image("instagram_icon").getAlt() }}"/></a>
				{% endif %}
			</div>
				  <div style="display:flex; justify-content:flex-end;gap:20px;">{% set sideNav = pimcore_build_nav({active: document, root: navStartNode, htmlMenuPrefix: 'my-nav-'}) %}
						{{ pimcore_render_nav(sideNav, 'menu', 'renderMenu', {
							ulClass: 'nav-links',
							expandSiblingNodesOfActiveBranch: true
						}) }}
