
	{% set menuRenderer = pimcore_nav_renderer('menu') %}
	{% set navStartNode = document.getProperty('navigationRoot') %}

	<nav class="navbar">
		<div class="container-fluid">
			<div class="logo"><a href ="/" style="padding:10px 70px;text-decoration: none;">Frank Mollers<br/>
			<p style ="padding:10px 70px;">Copyright © 2024</p>
			</div>
			<div class="social-icon">
				<a href="#"class="p-3">
					{% if editmode %}
						{{pimcore_image("facebook_icon", {
									    "title": "Drag your image here",
									    "width": 60,
									    "height": 60,
									    "thumbnail": "facebook_icon"
									}) }}
					{% else %}
						<img src="{{pimcore_image("facebook_icon").getSrc() }}" alt="{{pimcore_image("facebook_icon").getAlt() }}"/></a>
